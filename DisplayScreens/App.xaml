<Application
    x:Class="DisplayScreens.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:DisplayScreens"
    Startup="Application_Startup">
    <Application.Resources>
        <FontFamily x:Key="FontAwesomeBrands">/Fonts/fa-brands-400.ttf#Font Awesome 6 Brands Regular</FontFamily>
        <FontFamily x:Key="FontAwesomeDuotone">/Fonts/fa-duotone-900.ttf#Font Awesome 6 Duotone Solid</FontFamily>
        <FontFamily x:Key="FontAwesomeLight">/Fonts/fa-light-300.ttf#Font Awesome 6 Pro Light</FontFamily>
        <FontFamily x:Key="FontAwesomeRegular">/Fonts/fa-regular-400.ttf#Font Awesome 6 Pro Regular</FontFamily>
        <FontFamily x:Key="FontAwesomeSolid">/Fonts/fa-solid-900.ttf#Font Awesome 6 Pro Solid</FontFamily>
        <FontFamily x:Key="FontAwesomeThin">/Fonts/fa-thin-100.ttf#Font Awesome 6 Pro Thin</FontFamily>
    </Application.Resources>
</Application>
